{
  "version": "1.1.2-*",
  "name": "CoreCompat.Portable.Licensing",
  "authors": [
    "Nauck IT KG"
  ],
  "buildOptions": {
    "warningsAsErrors": true,
    "keyFile": "Portable.Licensing.snk",
    "strongName": true,
    "compile": {
      "includeFiles": [
        "../CommonAssemblyInfo.cs"
      ],
      "include": [
        "*.cs",
        "Properties/*.cs",
        "Security/**/*.cs",
        "Validation/*.cs"
      ]
    }
  },
  "frameworks": {
    "net451": {
      "dependencies": {
        "BouncyCastle": "1.8.1"
      },
      "frameworkAssemblies": {
        "System.Xml": "4.0.0",
        "System.Xml.Linq": "4.0.0",
        "System.IO": {
          "type": "build"
        },
        "System.Runtime": {
          "type": "build"
        },
        "System.Runtime.Extensions": {
          "type": "build"
        }
      }
    },
    "netstandard1.5": {
      "imports": "dotnet",
      "buildOptions": {
        "define": [
          "CORECLR"
        ]
      },
      "dependencies": {
        "System.Runtime": "4.1.0",
        "System.Runtime.Extensions": "4.1.0",
        "System.Xml.ReaderWriter": "4.0.11",
        "System.Xml.XDocument": "4.0.11",
        "System.Resources.ResourceManager": "4.0.1",
        "CoreCompat.BouncyCastle": "1.8.1-beta003"
      }
    }
  }
}